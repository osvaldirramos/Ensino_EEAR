    Transaction = DM.IBTransaction_Chamada
    AfterScroll = IBQuery_AlunoTurmaAfterScroll
    BufferChunks = 1000
    CachedUpdates = True
    DataSource = DST_Turma
    SQL.Strings = (
      
        'select a.idaluno, a.idcurso, a.idturma, a.identidade, a.codigoal' +
        'uno, a.desligado, a.observacao,'
      
        '       p.nomecompleto,  p.nomeguerra, p.postograduacao, p.especi' +
        'alidade'
      'from alunoturma a, pessoa p'
      'where a.identidade = p.identidade'
      'and     a.IdCurso = :IdCurso'
      'and     a.IdTurma = :IdTurma'
      'order by p.especialidade, p.nomeguerra')
