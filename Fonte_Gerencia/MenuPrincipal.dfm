  object IBQuery_ListaFaltasPorAlunos: TIBQuery
    Database = DM.IBDatabase_Chamada
    Transaction = DM.IBTransaction_Chamada
    BufferChunks = 1000
    CachedUpdates = True
    DataSource = DSQ_TurmaAndamento
    SQL.Strings = (
      
        'Select a.idcurso, a.idturma, a.serie, a.idaluno, a.especialidade' +
        ', a.nomecompleto, a.nomeguerra, a.identidade, a.Qtd_Faltas, t.te' +
        'mpo as tempo_espec,(a.Qtd_Faltas*100)/t.tempo as Por_Falta'
      'From  QTD_FALTA_TURMA_VIEW a, tempo_especialidade t'
      '                 where a.idcurso = t.idcurso'
      '                 and   a.idturma =  t.idturma'
      '                 and   a.serie = a.serie'
      '                 and   a.especialidade = t.especialidade'
      '                 and   a.idcurso = :idcurso '
      '                 and   a.idturma = :idturma '
      '                 and   a.serie = :serie '
      'order by a.Qtd_Faltas desc , a.nomeguerra')
    Left = 841
    Top = 320
    ParamData = <
      item
        DataType = ftSmallint
        Name = 'IDCURSO'
        ParamType = ptUnknown
        Size = 2
      end
      item
        DataType = ftInteger
        Name = 'IDTURMA'
        ParamType = ptUnknown
        Size = 4
      end
      item
        DataType = ftString
        Name = 'SERIE'
        ParamType = ptUnknown
        Size = 2
      end>
