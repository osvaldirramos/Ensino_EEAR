  object IBQuery_Faltas_Aluno: TIBQuery
    Database = DM.IBDatabase_Chamada
    Transaction = DM.IBTransaction_Chamada
    BufferChunks = 1000
    CachedUpdates = True
    DataSource = DSQ_ListaFaltasPorAlunos
    SQL.Strings = (
      
        'select a.idcurso, a.idturma, a.serie, a.idaluno, a.especialidade' +
        ', a.iddisciplina, a.codmateria, a.tempo_disciplina, a.Qtd_Faltas' +
        ', ((Qtd_Faltas/t.tempo)*100) as Por_Dis,'
      '(a.Qtd_Faltas*100)/t.tempo as Por_Geral'
      'From Qtd_Falta_View a, tempo_especialidade t'
      'where a.idcurso = :idcurso '
      'and   a.idturma = :idturma '
      'and   a.serie = :serie '
      'and   a.idaluno = :idaluno'
      'and   a.idcurso = t.idcurso'
      'and   a.idturma =  t.idturma'
      'and   a.serie = a.serie'
      'and   a.especialidade = t.especialidade'
      'order by a.codmateria')
    Left = 945
    Top = 336
    ParamData = <
      item
        DataType = ftSmallint
        Name = 'IDCURSO'
